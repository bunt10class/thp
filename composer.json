{
  "name": "bunt10class/thp",
  "description": "type hinting for php functions",
  "version": "1.0.0",
  "type": "library",
  "keywords": ["php", "type hinting"],
  "license": "GPL",
  "config": {
    "sort-packages": true
  },
  "require": {
    "php": ">=8.1"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.5",
    "squizlabs/php_codesniffer": "3.*"
  },
  "autoload": {
    "psr-4": {
      "THP\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  }
}